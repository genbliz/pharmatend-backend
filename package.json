{
  "name": "tend-pos",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-raw-dev": "rimraf ./dist && tsc -b",
    "build-dev": "webpack --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
    "build-bin": "webpack --config webpack.bin.js",
    "server": "node ./dist-dev/start-dev.js",
    "pkg:0": "pkg package.json",
    "pkg": "node ./scripts/pkg.js",
    "ncc": "node ./scripts/ncc.js",
    "caxa": "node ./scripts/caxa.js",
    "dist-bin": "rimraf dist && npm run build-prod && npm run ncc && npm run caxa",
    "dist-bin2": "rimraf dist && npm run build-prod && npm run ncc && npm run pkg",
    "lint-fix": "eslint . --ext .ts,.tsx,.js --fix",
    "lint": "eslint --config ./.eslintrc.js --ext .ts src/**/*.ts",
    "deploy-dev": "sls deploy --stage dev --env dev",
    "deploy-prod": "sls deploy --stage prod --env prod",
    "=====": "=======",
    "start-debug": "ts-node --inspect=5858 --debug-brk --ignore false src/server.ts",
    "start:ts": "ts-node -r tsconfig-paths/register src/server.ts",
    "seed": "ts-node -r tsconfig-paths/register src/app/seed/index.ts",
    "start:x": "ts-node src/server.ts",
    "start": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "start00": "ts-node -r tsconfig-paths/register src/server.ts",
    "startmon": "nodemon --watch src/**/*.ts --ignore src/**/*.spec.ts --exec ts-node -r tsconfig-paths/register src/server.ts",
    "prettier-format-fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "prettier-different": "prettier --list-different \"src/**/*.{ts,js,json}\"",
    "prettier-unformatted": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.272.0",
    "@aws-sdk/s3-request-presigner": "^3.272.0",
    "app-root-path": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "busboy": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dottie": "^2.0.3",
    "express": "^4.18.2",
    "get-stream": "^6.0.1",
    "helmet": "^6.0.1",
    "hospiman-aes-crypto": "^1.1.6",
    "ini": "^3.0.1",
    "ip": "^2.0.0",
    "is-online": "^9.0.1",
    "joi": "^17.7.1",
    "jsbarcode": "^3.11.5",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.10.19",
    "lodash": "^4.17.21",
    "mocody": "^0.4.26",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.4",
    "node-fetch": "^2.6.9",
    "papaparse": "^5.3.2",
    "qrcode": "^1.5.1",
    "sqlite3": "^5.1.4",
    "sudo-prompt": "^9.2.1",
    "validator": "^13.9.0",
    "xmldom": "^0.6.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.8",
    "@types/busboy": "^1.5.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dottie": "^2.0.4",
    "@types/express": "^4.17.17",
    "@types/ip": "^1.1.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "^2.6.2",
    "@types/papaparse": "^5.3.7",
    "@types/qrcode": "^1.5.0",
    "@types/validator": "^13.7.12",
    "@types/xmldom": "^0.1.31",
    "@vercel/ncc": "^0.36.1",
    "caxa": "^3.0.1",
    "pkg": "^5.8.0",
    "rimraf": "^4.1.2",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "pkg": {
    "scripts": "dist/server.js",
    "assets": [
      "node_modules/leveldown/prebuilds/**/*",
      "node_modules/sqlite3/lib/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist-bin"
  }
}
