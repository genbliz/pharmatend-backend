{
  "compilerOptions": {
    "target": "es2022",
    "module": "nodenext",
    "moduleResolution": "node16",
    "lib": ["dom", "esnext"],
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "importHelpers": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "erasableSyntaxOnly": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "useDefineForClassFields": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "exactOptionalPropertyTypes": false,
    "useUnknownInCatchVariables": false,
    "verbatimModuleSyntax": false,
    "noImplicitThis": true,
    "strict": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "outDir": "./build-dev",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/app/*"],
      "#cdk/*": ["./cdk/*"],
      "#func/*": ["./src/func/*"]
    }
  },
  "include": ["./src/**/*", "./cdk/**/*"],
  "exclude": [
    "node_modules",
    "build-dev",
    "build",
    "dist-func",
    "dist",
    ".cache",
    "dumps",
    "cdk.out",
    "./src/**/*.test.ts",
    "./src/**/*.spec.ts"
  ]
}
